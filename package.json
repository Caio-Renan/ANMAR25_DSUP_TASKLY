    "generate:dev": "dotenv -e .env.development -- npx prisma generate",
    "generate:prod": "dotenv -e .env.production -- npx prisma generate",
    "status": "dotenv -e .env.development -- npx prisma migrate status",
    "build": "tsc",
    "start": "dotenv -e .env.production -- node dist/server.js",
    "start:prod": "npm run build && npm start",
    "studio": "dotenv -e .env.development -- npx prisma studio",
